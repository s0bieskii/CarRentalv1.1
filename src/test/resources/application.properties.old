spring.datasource.url=jdbc:h2:~/car-rental-tests-db;DB_CLOSE_ON_EXIT=FALSE;DB_CLOSE_DELAY=-1;AUTO_SERVER=TRUE;FILE_LOCK=SOCKET;
spring.datasource.driverClassName=org.h2.Driver
